{
  "expo": {
    "name": "Gallery Pro",
    "slug": "gallery-pro",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./icon.png",
    "userInterfaceStyle": "dark",
    "plugins": [
      "expo-image-picker",
      "expo-screen-orientation",
      "expo-navigation-bar"
    ],
    "android": {
      "package": "com.yourname.gallerypro",
      "permissions": ["READ_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./icon.png",
        "backgroundColor": "#000000"
      }
    }
  }
}
